<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- xml mapper와 짝이 될 java mapper의 패키지명(경로명)과 파일명을 적어줌 -->
<mapper namespace="com.demo.mapper.LoginMapper">
	<select id="checkLogin" resultType="Member" parameterType="String">
		select * from member where email = #{param1} and password = #{password}
	</select>  	
	
</mapper>


